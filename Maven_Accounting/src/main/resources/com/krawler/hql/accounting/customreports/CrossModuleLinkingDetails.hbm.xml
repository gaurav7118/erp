<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.krawler.spring.accounting.customreports.CrossModuleLinkingDetails" table="crossmodulelinkingdetails">
    <id name="id" type="string">
        <column name="id"/>
        <generator class="uuid"/>
    </id>
    <property name="module" type="string">
        <column name="module"/>
    </property>
    <property name="linkedmodule" type="string">
        <column name="linkedmodule"/>
    </property>
    <property name="allowcrossmodule" type="boolean">
        <column name="allowcrossmodule"/>
    </property>
   
</class>
</hibernate-mapping>

