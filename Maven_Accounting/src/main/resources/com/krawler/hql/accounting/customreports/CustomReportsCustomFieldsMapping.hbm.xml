<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.krawler.spring.accounting.customreports.AccCustomReportsCustomFieldsMapping" table="customreportscustomfieldsmapping">
    <id name="id" type="string">
        <column name="id"/>
        <generator class="uuid"/>
    </id>
    <property name="module" type="string">
        <column name="module"/>
    </property>
    <property name="defaultheaderid" type="string">
        <column name="defaultheaderid"/>
    </property>
    <property name="islineitem" type="boolean">
        <column name="islineitem"/>
    </property>
    <property name="isDefaultItem" type="boolean">
        <column name="isdefaultitem"/>
    </property>
    <property name="mappingFor" type="string">
        <column name="mappingfor"/>
    </property>
</class>
</hibernate-mapping>

