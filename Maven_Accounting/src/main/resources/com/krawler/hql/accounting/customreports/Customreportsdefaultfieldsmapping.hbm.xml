<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.krawler.spring.accounting.customreports.AccCustomReportsDefaultFieldsMapping" table="customreportsdefaultfieldsmapping">
    <id name="id" type="string">
      <column name="id"/>
      <generator class="uuid"/>
    </id>
    <property name="defaultfieldid" type="string">
      <column name="defaultfieldid"/>
    </property>
    <property name="defaultheaderid" type="string">
      <column name="defaultheaderid"/>
    </property>
    <property name="isSelectDataIndex" type="boolean">
      <column name="isselectdataindex" />
    </property>
  </class>
</hibernate-mapping>
