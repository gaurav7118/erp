<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
	<rule>
		<from>/a/([^\/]*)/(.*)</from>
		<to>/$2?cdomain=$1</to>
	</rule>
	<rule>
		<from>/b/([^\/]*)/(.*)</from>
		<to>/$2?cdomain=$1</to>
	</rule>
</urlrewrite>