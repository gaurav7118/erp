<settings>
    <thousands_separator>,</thousands_separator>
  <decimals_separator>.</decimals_separator>
  <digits_after_decimal>2</digits_after_decimal>
  <precision>2</precision>
  <redraw>1</redraw>
  <background>
    <alpha>100</alpha>
    <border_alpha>100</border_alpha>
  </background>
  <balloon>
    <show>{title}: {value}</show>
  </balloon>
  <legend>
    <x>393</x>
    <y>80</y>
    <width>180</width>
    <max_columns>1</max_columns>
    <values>
      <enabled>1</enabled>
      <width>84</width>
      <text>{value}</text>
    </values>
  </legend>
  <pie>
    <x>30%</x>
    <y>60%</y>
    <height>20</height>
    <angle>30</angle>
    <gradient>radial</gradient>
    <gradient_ratio>0,0,0,0</gradient_ratio>
  </pie>
  <animation>
    <start_time>2</start_time>
    <pull_out_time>1.5</pull_out_time>
  </animation>
  <data_labels>
    <show>{title}</show>
     <hide_labels_percent>100</hide_labels_percent>
  </data_labels>
  <labels>
