<settings>
            <thousands_separator>,</thousands_separator>
            <decimals_separator>.</decimals_separator>
     <digits_after_decimal>0</digits_after_decimal>
  <redraw>1</redraw>
  <data_type>csv</data_type>
  <background>
    <alpha>100</alpha>
    <border_alpha>70</border_alpha>
  </background>
  <plot_area>
    <margins>
      <left>116</left>
      <top>100</top>
      <bottom>150</bottom>
    </margins>
  </plot_area>
  <grid>
    <category>
      <alpha>5</alpha>
      <dash_length>1</dash_length>
    </category>
    <value>
      <alpha>7</alpha>
    </value>
  </grid>
  <axes>
    <category>
      <width>1</width>
    </category>
    <value>
      <width>1</width>
    </value>
  </axes>
  <values>
    <value>
      <min>0</min>
    </value>
  </values>
  <depth>15</depth>
  <column>
    <type>stacked</type>
    <width>45</width>
    <balloon_text>{title}: {value}</balloon_text>
    <grow_time>3</grow_time>
  </column>
  <graphs>
    <graph gid="0">
      <title>Received</title>
      <color>992222</color>
      <alpha>50</alpha>
    </graph>
    <graph gid="1">
      <title>Due</title>
      <color>FF9966</color>
      <alpha>60</alpha>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
      <text><![CDATA[<b><u>Amount Receivable in the Financial Year </u></b>]]></text>
      <y>18</y>
      <text_color>660033</text_color>
      <text_size>15</text_size>
      <align>center</align>
    </label>
    <label lid="1">
      <text><![CDATA[<b>Amount</b>]]></text>
         <x>20</x>
      <y>200</y>
      <text_color>3399FF</text_color>
      <text_size>13</text_size>
      <rotate>1</rotate>
    </label>
    <label lid="2">
      <text><![CDATA[<b>Month</b>]]></text>
      <x>500</x>
      <y>520</y>
      <text_color>3399FF</text_color>
      <text_size>13</text_size>
    </label>
  </labels>
</settings>