<settings>
  <thousands_separator>,</thousands_separator>
  <decimals_separator>.</decimals_separator>
  <redraw>1</redraw>
  <data_type>xml</data_type>
  <background>
    <alpha>100</alpha>
    <border_alpha>20</border_alpha>
  </background>
  <plot_area>
    <margins>
      <left>116</left>
      <top>100</top>
      <bottom>150</bottom>
    </margins>
  </plot_area>
  <grid>
    <category>
      <alpha>5</alpha>
      <dash_length>1</dash_length>
    </category>
    <value>
      <alpha>7</alpha>
    </value>
  </grid>
  <axes>
    <category>
      <width>1</width>
    </category>
    <value>
      <width>1</width>
    </value>
  </axes>
  <values>
    <value>
      <min>0</min>
    </value>
  </values>
  <depth>15</depth>
  <column>
    <width>45</width>
    <balloon_text>{title}: {value}</balloon_text>
    <grow_time>3</grow_time>
  </column>
  <graphs>
    <graph gid="0">
      <title>Transaction Amount</title>
      <color>CC6699</color>
      <alpha>97</alpha>
    </graph>
    <graph gid="1">
      <title>Column</title>
      <color>7F8DA9</color>
    </graph>
    <graph gid="2">
      <title>Line</title>
      <color>FEC514</color>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
      <text><![CDATA[<b><u>Our Key Customers</u></b>]]></text>
      <y>18</y>
      <text_color>660033</text_color>
      <text_size>15</text_size>
      <align>center</align>
    </label>
    <label lid="1">
      <text><![CDATA[<b>Amount</b>]]></text>
         <x>20</x>
      <y>200</y>
      <text_color>CC6666</text_color>
      <text_size>13</text_size>
      <rotate>1</rotate>
    </label>
  </labels>
</settings>
