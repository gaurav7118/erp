<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.krawler.common.admin.FieldComboData" table="fieldcombodata">
        <id name="id" type="string">
            <column name="id"/>
            <generator class="assigned"/>
        </id>
        <property name="value" type="string">
            <column name="value"/>
        </property>
        <set cascade="all" inverse="true" name="children">
            <key column="parent"/>
            <one-to-many class="com.krawler.common.admin.FieldComboData"/>
        </set>
        <property name="fieldid" type="string">
            <column name="fieldid"/>
        </property>
        <property name="projectid" type="string">
            <column name="projectid"/>
        </property>
        <property name="eclaimid" type="string">
            <column name="eclaimid"/>
        </property>
        <property name="taskid" type="string">
            <column name="taskid"/>
        </property>
        <property name="oldid" type="int">
            <column default="0" name="oldid"/>
        </property>
        <property name="deleteflag" type="integer">
            <column default="0" name="deleteflag"/>
        </property>
        <property name="itemsequence" type="integer">
            <column default="0" name="itemsequence"/>
        </property>
        <property name="itemdescription" type="string">
            <column name="itemdescription"/>
        </property>
         <property name="activatedeactivatedimensionvalue" type="true_false">
            <column name="activatedeactivatedimensionvalue" default="true"/>
        </property>
        <property name="salesCommissionSchemaMaster" type="string">
            <column name="salescommissionschemamaster"/>
        </property>
        <property name="seqnumber" type="integer">
            <column name="seqnumber" default='0'/>
        </property>
        <property name="autoGenerated" type="true_false">
            <column default="false" name="autogen" not-null="true"/>
        </property>
        <property name="valueType" type="int">
            <column default="0" name="valuetype"/>
        </property>
        <many-to-one class="com.krawler.hql.accounting.SequenceFormat" column="seqformat" name="seqformat"/>
        <many-to-one class="com.krawler.common.admin.FieldParams" column="fieldid" insert="false" name="field" update="false"/>
        <many-to-one class="com.krawler.common.admin.FieldComboData" name="parent"/>
        <many-to-one class="com.krawler.common.admin.FieldComboData" name="propagatedfieldcomboID" column="parentcofieldcomboid"/>
    </class>
</hibernate-mapping>
