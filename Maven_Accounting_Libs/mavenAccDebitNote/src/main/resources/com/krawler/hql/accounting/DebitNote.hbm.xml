<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.krawler.hql.accounting.DebitNote" table="debitnote">
        <id column="id" name="ID">
            <generator class="uuid"/>
        </id>
        <property name="sequence" type="integer">
            <column name="seq" not-null="true"/>
        </property>
        <property name="debitNoteNumber" type="string">
            <column length="50" name="dnnumber" not-null="true"/>
        </property>
        <property name="autoGenerated" type="true_false">
            <column default="false" name="autogen" not-null="true"/>
        </property>
        <property name="memo" type="string">
            <column length="2048" name="memo"/>
        </property>
        <property name="externalCurrencyRate" type="double">
            <column name="externalcurrencyrate" not-null="true" default='0'/>
        </property>
        <property name="deleted" type="true_false">
            <column name="deleteflag" not-null="true"/>
        </property>
        <many-to-one class="com.krawler.hql.accounting.Vendor" column="vendor" name="vendor"/>
        <many-to-one class="com.krawler.hql.accounting.Customer" column="customer" name="customer"/>
        <property name="otherwise" type="true_false">
            <column name="otherwise" not-null="true"/>
        </property>
        <property name="openflag" type="true_false">
            <column name="openflag" not-null="true"/>
        </property>
        <property name="dnamount" type="double">
            <column name="dnamount" default='0'/>
        </property>
        <property name="dnamountdue" type="double">
            <column name="dnamountdue" default='0'/>
        </property>
        
        <property name="includingGST" type="true_false">
            <column name="includinggst" default="false"/>
        </property>
        <property name="dntype" type="integer">
            <column name="dntype" />
        </property>
        <property name="approvestatuslevel" type="integer">
            <column name="approvestatuslevel" default='0'/>
        </property>
        <property name="seqnumber" type="integer">
            <column name="seqnumber" default='0'/>
        </property>
        <property name="datePreffixValue" type="string">
            <column length="50" name="datepreffixvalue"/>
        </property>
        <property name="dateSuffixValue" type="string">
            <column length="50" name="datesuffixvalue"/>
        </property>
        <property name="dateAfterPreffixValue" type="string">
            <column length="50" name="dateafterpreffixvalue"/>
        </property>
        <property name="revalJeId" type="string">
            <column name="revaljeid"/>
        </property>
        <property name="createdon" type="long">
            <column name="createdon" not-null="true"/>
        </property>
    
        <property name="isOpeningBalenceDN" type="boolean">
            <column  name="isopeningbalencedn"/>
        </property>
        <property name="dNForVendor" type="boolean">
            <column  name="isdnforvendor"/>
        </property>
        <property name="normalDN" type="boolean" >
            <column  name="isnormaldn" not-null="true" default="true"/>
        </property>
        <property name="openingBalanceAmountDue" type="double">
            <column  name="openingbalanceamountdue"/>
        </property>
        <property name="openingBalanceBaseAmountDue" type="double">
            <column  name="openingbalancebaseamountdue"/>
        </property>
        <property name="originalOpeningBalanceBaseAmount" type="double">
            <column  name="originalopeningbalancebaseamount"/>
        </property>
        <property name="creationDate" type="date">
            <column name="creationdate"/>
        </property>
        <property name="narration" type="string">
            <column name="narration"/>
        </property>
        <property name="exchangeRateForOpeningTransaction" type="double">
            <column  name="exchangerateforopeningtransaction"/>
        </property>
        <property name="conversionRateFromCurrencyToBase" type="boolean">
            <column  name="isconversionratefromcurrencytobase"/>
        </property>
        <property name="updatedon" type="long">
            <column name="updatedon" not-null="true"/>
        </property>
        <property name="oldRecord" type="boolean" >
            <column  name="isoldrecord" default="false"/>
        </property>
        <property name="printed" type="boolean">
            <column name="printedflag" not-null="false" default="false"/>
        </property>
        <property name="dnamountinbase" type="double">
            <column name="dnamountinbase" default='0'/>
        </property>
        <property name="supplierInvoiceNo" type="string">
            <column  name="supplierinvoiceno" default=""/>
        </property>
        <property name="mvatTransactionNo" type="string">
            <column  name="mvattransactionno" default=""/>
        </property>
        <many-to-one class="com.krawler.hql.accounting.SequenceFormat" column="seqformat" name="seqformat"/>
        <set cascade="all" inverse="true" name="discounts">
            <key column="debitnote"/>
            <one-to-many class="com.krawler.hql.accounting.DebitNoteDiscount"/>
        </set>
        <set cascade="all" inverse="true" name="rows">
            <key column="debitnote"/>
            <one-to-many class="com.krawler.hql.accounting.DebitNoteDetail"/>
        </set>
        <set cascade="all" inverse="true" name="dnTaxEntryDetails">
            <key column="debitnote"/>
            <one-to-many class="com.krawler.hql.accounting.DebitNoteTaxEntry"/>
        </set>
        
         <set cascade="all" inverse="true" name="rowsGst">
            <key column="debitnote"/>
            <one-to-many class="com.krawler.hql.accounting.DebitNoteAgainstCustomerGst"/>
        </set>
        <property name="tdsPaidFlag" type="integer">
            <column name="tdspaidflag" default="0"/>
        </property>
        <property name="tdsPayment" type="string">
            <column name="tdspayment"/>
        </property>
        <property name="tdsInterestPaidFlag" type="integer">
            <column name="tdsinterestpaidflag" default="0"/>
        </property>
        <property name="tdsInterestPayment" type="string">
            <column name="tdsinterestpayment"/>
        </property>
        <property name="gstCurrencyRate" type="double">
            <column name="gstCurrencyRate"/>
        </property>
        <many-to-one class="com.krawler.common.admin.KWLCurrency" column="currency" name="currency" not-null="true"/>
        <many-to-one class="com.krawler.hql.accounting.Tax" column="tax" name="tax"/>
        <many-to-one cascade="all" class="com.krawler.hql.accounting.JournalEntry" column="journalentry" name="journalEntry" unique="true"/>
        <many-to-one cascade="all" class="com.krawler.hql.accounting.JournalEntryDetail" column="centry" name="vendorEntry" unique="true"/>
        <many-to-one class="com.krawler.common.admin.Company" name="company" not-null="true"/>
        <many-to-one class="com.krawler.hql.accounting.Account" column="account" name="account"/>
        <many-to-one class="com.krawler.hql.accounting.PurchaseReturn" column="purchasereturn" name="purchaseReturn"/>
        <many-to-one column="accopeningbalancedebitnotecustomdataref" class="com.krawler.hql.accounting.OpeningBalanceDebitNoteCustomData"  name="openingBalanceDebitNoteCustomData" />
        <many-to-one class="com.krawler.common.admin.User" column="createdby" name="createdby"/>
        <many-to-one class="com.krawler.common.admin.User" column="modifiedby" name="modifiedby"/>
        <many-to-one class="com.krawler.common.admin.CostCenter" column="costcenter" name="costcenter"/>
        <many-to-one class="com.krawler.common.admin.BillingShippingAddresses" column="billingshippingaddresses" name="billingShippingAddresses"/>
        <many-to-one class="com.krawler.hql.accounting.MasterItem" column="salesperson" name="salesPerson"/>
        <many-to-one class="com.krawler.hql.accounting.MasterItem" column="agent" name="agent"/>
    </class>
</hibernate-mapping>
