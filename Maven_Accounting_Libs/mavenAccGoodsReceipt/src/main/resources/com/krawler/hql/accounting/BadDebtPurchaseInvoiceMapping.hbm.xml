<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.krawler.hql.accounting.BadDebtPurchaseInvoiceMapping" table="baddebtpurchaseinvoicemapping">
        <id column="id" name="id">
            <generator class="uuid"/>
        </id>
        <property name="badDebtType" type="integer">
            <column name="baddebttype"/>
        </property>
        <property name="badDebtAmtRecovered">
            <column name="baddebtamtrecovered"/>
        </property>
        <property name="badAmtDebtGSTAmtRecovered">
            <column name="badamtdebtgstamtrecovered"/>
        </property>
        <property name="badDebtAmtClaimed">
            <column name="baddebtamtclaimed"/>
        </property>
        <property name="badDebtGSTAmtClaimed">
            <column name="baddebtgstamtclaimed"/>
        </property>
        <property name="badDebtClaimedDate" type="timestamp">
            <column name="baddebtclaimeddate"/>
        </property>
        <property name="badDebtRecoveredDate" type="timestamp">
            <column name="baddebtrecovereddate"/>
        </property>
          <property name="seqnumber" type="integer">
            <column name="seqnumber"/>
        </property>
        <property name="datePreffixValue" type="string">
            <column length="50" name="datepreffixvalue"/>
        </property>
        <property name="dateSuffixValue" type="string">
            <column length="50" name="datesuffixvalue"/>
        </property>
        <property name="dateAfterPreffixValue" type="string">
            <column length="50" name="dateafterpreffixvalue"/>
        </property>
        <property name="badDebtSeqNumber" type="string">
            <column name="baddebtseqnumber"/>
        </property>
        <property name="paymentId" type="string">
            <column name="paymentid"/>
        </property>
        <many-to-one class="com.krawler.hql.accounting.SequenceFormat" column="seqformat" name="seqformat"/>
        
        <property name="autoGenerated" type="true_false">
            <column default="false" name="autogen" />
        </property>
        <many-to-one class="com.krawler.hql.accounting.GoodsReceipt" column="goodsreceipt" name="goodsReceipt"/>
        <many-to-one class="com.krawler.hql.accounting.JournalEntry" column="journalentry" name="journalEntry"/>
        <many-to-one class="com.krawler.common.admin.Company" name="company" not-null="true"/>
    </class>
  

</hibernate-mapping>
