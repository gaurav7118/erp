<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.krawler.hql.accounting.DealerExciseDetails" table="dealerexcisedetails">
    <id name="id" column="id" type="string">
        <generator class="uuid"/>
    </id>
    <property name="RG23DEntryNumber" type="string">
        <column name="RG23DEntryNumber" default=""/>
    </property>
    <property name="SupplierRG23DEntry" type="string">
        <column name="supplierRG23DEntry" default=""/>
    </property>
    <property name="AssessableValue" type="string">
        <column name="assessableValue" default=""/>
    </property>
    <property name="PLARG23DEntry" type="string">
        <column name="PLARG23DEntry" default=""/>
    </property>
    <property name="ManuAssessableValue" type="string">
        <column  name="manuAssessableValue" default=""/>
    </property>
    <property name="seqnumber" type="integer">
        <column name="seqnumber" default="0"/>
    </property>
    <property name="dateSuffixValue" type="string">
        <column name="datesuffixvalue" default=""/>
    </property>
    <property name="dateAfterPreffixValue" type="string">
        <column length="50" name="dateafterpreffixvalue"/>
    </property>
    <property name="datePreffixValue" type="string">
        <column name="datepreffixvalue" default=""/>
    </property>
    <property name="autoGenerated" type="true_false">
        <column name="autogen" default="false"/>
    </property>
    <property name="invoicenoManufacture" type="string">
        <column name="manufactureinvoiceno" default=""/>
    </property>
    <property name="invoiceDateManufacture" type="date">
        <column name="manufactureinvoicedate"/>
    </property>
    <many-to-one class="com.krawler.common.admin.Company" column="company" name="company"/>
    <many-to-one class="com.krawler.hql.accounting.SequenceFormat" column="seqformat" name="seqformat"/>
    <many-to-one class="com.krawler.hql.accounting.GoodsReceiptDetail" column="goodsreceiptdetails" name="goodsreceiptdetails"/>
    </class>
</hibernate-mapping>
