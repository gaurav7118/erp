<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.krawler.hql.accounting.DeliveryOrder" table="deliveryorder">
        <id column="id" name="ID">
            <generator class="uuid"/>
        </id>
        <property name="deliveryOrderNumber" type="string">
            <column length="50" name="donumber" not-null="true"/>
        </property>
        <property name="autoGenerated" type="true_false">
            <column default="false" name="autogen" not-null="true"/>
        </property>
        <property name="orderDate" type="date">
            <column name="orderdate" not-null="true"/>
        </property>
        <property name="memo" type="string">
            <column length="2048" name="memo"/>
        </property>
        <property name="orderNoReferedForMemoAddress" type="string">
            <column length="255" name="ordernoreferedformemoaddress"/>
        </property>
        <property name="postText" type="string">
            <column  name="posttext"/>
        </property>
        <property name="customerPORefNo" type="string">
            <column length="50" name="customerporefno"/>
        </property>      
        <property name="deleted" type="true_false">
            <column name="deleteflag" not-null="true"/>
        </property>
        <property name="shipvia" type="string">
            <column length="255" name="shipvia"/>
        </property>
        <property name="fob" type="string">
            <column length="255" name="fob"/>
        </property>
        <property name="shipdate" type="date">
            <column name="shipdate"/>
        </property>
        <property name="favourite" type="boolean">
            <column name="favouriteflag" not-null="false" default="false"/>
        </property>
        <property name="fixedAssetDO" type="boolean">
            <column name="isfixedassetdo" default="false"/>
        </property>
        <property name="externalCurrencyRate" type="double">
            <column name="externalcurrencyrate" default="0"/>
        </property>
        <property name="printed" type="boolean">
            <column name="printedflag" not-null="false" default="false"/>
        </property>
        <property name="seqnumber" type="integer">
            <column name="seqnumber" default='0'/>
        </property>
        <property name="datePreffixValue" type="string">
            <column length="50" name="datepreffixvalue"/>
        </property>
        <property name="dateSuffixValue" type="string">
            <column length="50" name="datesuffixvalue"/>
        </property>
        <property name="dateAfterPreffixValue" type="string">
            <column length="50" name="dateafterpreffixvalue"/>
        </property>
        <property name="createdon" type="long">
            <column name="createdon" not-null="true"/>
        </property>
        <property name="updatedon" type="long">
            <column name="updatedon" not-null="true"/>
        </property>
        <property name="leaseDO" type="boolean">
            <column name="isleasedo"/>
        </property>
        <property name="appliedForTax" type="boolean">
            <column name="isappliedfortax"/>
        </property>
        <property name="isAutoGeneratedDO" type="true_false">
            <column default="false" name="isautogenerateddo" />
        </property>
        <property name="approvestatuslevel" type="integer">
            <column name="approvestatuslevel" not-null="true" default='0'/>
        </property>
        <property name="isconsignment" type="true_false">
            <column name="isconsignment" default="'F'" />
        </property>
        <property name="isJobWorkOut" type="true_false">
            <column name="isjobworkout" default="'F'" />
        </property>
        <property name="isDOClosed" type="true_false">
            <column name="isdoclosed" default="'F'" />
        </property>
        <property name="POSDO" type="boolean">
            <column name="isfromPOS"/>
        </property>
        <property name="isOpenInSI" type="true_false">
            <column name="isOpeninsi" default="'T'"/>
        </property>
        <property name="isOpenInSR" type="true_false">
            <column name="isopeninsr" default="'T'"/>
        </property>
        <property name="discountinbase" type="double">
            <column  name="discountinbase"/>
        </property>
        <property name="totalamount" type="double">
            <column  name="totalamount"/>
        </property>
        <property name="totalamountinbase" type="double">
            <column  name="totalamountinbase"/>
        </property>
        <property name="termsincludegst" type="true_false">
            <column name="termsincludegst"/>
        </property>
        <property name="formtype" type="string">
            <column  name="formtype"/>
        </property>
         <property name="gstIncluded" type="true_false">
            <column name="gstincluded" default="'F'" />
        </property>
        <property name="isFreeGift" type="boolean">
            <column name="isfreegift" default="false"/>
        </property>
        <property name="generatedSource" type="integer">
            <column name="generatedsource" not-null="false" default='0'/>
        </property>
        <property name="isewayexported" type="true_false">
            <column default="false" length="1" name="isewayexported"/>
        </property>
        <many-to-one class="com.krawler.hql.accounting.SequenceFormat" column="seqformat" name="seqformat"/>
        <set cascade="all" inverse="true" name="rows">
            <key column="deliveryorder"/>
            <one-to-many class="com.krawler.hql.accounting.DeliveryOrderDetail"/>
        </set>
        <set cascade="all" inverse="true" name="dOContractMappings">
            <key column="deliveryorder"/>
            <one-to-many class="com.krawler.hql.accounting.DOContractMapping"/>
        </set>
        <property name="appliedDate" type="date">
            <column name="applieddate" />
        </property>
        <property name="applyTaxToTerms" type="true_false">
            <column name="applytaxtoterms" default="'F'" />
        </property>
          <property name="isIndGSTApplied" type="true_false">
            <column name="isindgstapplied" default="'F'" />
        </property>
        <property name="rcmapplicable" type="true_false">
            <column name="rcmapplicable" default="'F'" />
        </property>
        <property name="ewayapplicable" type="true_false">
            <column name="ewayapplicable" default="'F'" />
        </property>
         <property name="isMerchantExporter" type="true_false">
            <column name="ismerchantexporter" default="'F'" />
        </property>
        <property name="isRoundingAdjustmentApplied" type="true_false">
            <column name="isroundingadjustmentapplied" default="'F'" />
        </property>
        <property name="roundingadjustmentamountinbase" type="double">
            <column  name="roundingadjustmentamountinbase"/>
        </property>
        <property name="roundingadjustmentamount" type="double">
            <column  name="roundingadjustmentamount"/>
        </property>
        <many-to-one class="com.krawler.common.admin.CostCenter" name="costcenter"/>
        <many-to-one class="com.krawler.hql.accounting.MasterItem" name="status" column="status"/>    
        <many-to-one cascade="all" class="com.krawler.hql.accounting.Customer" column="customer" name="customer" not-null="true"/>
        <many-to-one class="com.krawler.common.admin.Company" name="company" not-null="true"/>
        <many-to-one class="com.krawler.common.admin.Projreport_Template" name="templateid"/>
        <many-to-one column="accdeliveryordercustomdataref" class="com.krawler.hql.accounting.DeliveryOrderCustomData"  name="deliveryOrderCustomData" />
        <many-to-one class="com.krawler.hql.accounting.Tax" column="tax" name="tax"/>
        <many-to-one class="com.krawler.common.admin.User" column="createdby" name="createdby"/>
        <many-to-one class="com.krawler.common.admin.User" column="modifiedby" name="modifiedby"/>
        <many-to-one cascade="all" class="com.krawler.hql.accounting.JournalEntry" column="journalentry" name="journalEntry" unique="true"/>
        <many-to-one class="com.krawler.common.admin.KWLCurrency" column="currency" name="currency"/>
        <many-to-one class="com.krawler.common.admin.InventoryWarehouse" name="custWarehouse"/>
        <many-to-one class="com.krawler.hql.accounting.MasterItem" column="driver" name="driver"/>
        <many-to-one class="com.krawler.hql.accounting.MasterItem" column="movementtype" name="MovementType"/>
        <many-to-one class="com.krawler.common.admin.BillingShippingAddresses" column="billingshippingaddresses" name="billingShippingAddresses"/>
        <many-to-one class="com.krawler.hql.accounting.MasterItem" name="salesperson" column="salesperson"/>
        <many-to-one cascade="all" class="com.krawler.hql.accounting.JournalEntry" column="inventoryje" name="inventoryJE"/>
        <many-to-one class="com.krawler.hql.accounting.Term" name="term"/>
        <many-to-one cascade="all" class="com.krawler.hql.accounting.JournalEntry" column="freegiftje" name="freeGiftJE" unique="true"/>
    </class>
</hibernate-mapping>
