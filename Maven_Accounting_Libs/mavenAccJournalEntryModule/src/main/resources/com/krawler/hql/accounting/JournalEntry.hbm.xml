<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.krawler.hql.accounting.JournalEntry" table="journalentry">
        <id column="id" name="ID">
            <generator class="uuid"/>
        </id>
        <property name="entryNumber" type="string">
            <column length="50" name="entryno" not-null="true"/>
        </property>
        <property name="autoGenerated" type="true_false">
            <column default="false" name="autogen" not-null="true"/>
        </property>
        <property name="entryDate" type="date">
            <column name="entrydate" not-null="true"/>
        </property>
        <property name="createdOn" type="long">
            <column name="createdon"/>
        </property>
        <property name="memo" type="string">
            <column length="2048" name="memo"/>
        </property>
        <property name="chequeprinted" type="boolean">
            <column name="chequeprinted" default="false"/>
        </property>
        <property name="externalCurrencyRate" type="double">
            <column default="0" name="externalcurrencyrate" not-null="true"/>
        </property>
        <property name="deleted" type="true_false">
            <column name="deleteflag" not-null="true"/>
        </property>
        <property name="eliminateflag" type="true_false">
            <column name="eliminateflag" not-null="true"/>
        </property>
        <property name="intercompanyflag" type="true_false">
            <column name="intercompanyflag" />
        </property>
        <property name="isFromPOS" type="boolean">
            <column name="isfromPOS"/>
        </property>
        <property name="isFromEclaim" type="boolean">
            <column name="isfromeclaim"/>
        </property>
        <property name="optimizedflag" type="true_false">
            <column name="optimizedflag" not-null="false"/>
        </property>
        <set cascade="all-delete-orphan"  inverse="true" name="details">
            <key column="journalEntry"/>
            <one-to-many class="com.krawler.hql.accounting.JournalEntryDetail"/>
        </set>
        <property name="isInventory" type="string">
            <column length="2" name="isinventory" />
        </property>
        <property name="isReverseJE" type="true_false">
            <column name="isreverseje" not-null="true"/>
        </property>
        <property name="isOneTimeReverse" type="true_false">
            <column name="isOneTimeReverse" not-null="true"/>
        </property>
        <property name="reverseJournalEntry" type="string">
            <column length="255" name="reversejournalentry"/>
        </property>
        <property name="pendingapproval" type="integer">
            <column default="0" name="pendingapproval" not-null="true"/>
        </property>
        <property name="isReval" type="integer">
            <column default="0" name="isReval" not-null="true"/>
        </property>
        <property name="istemplate" type="integer">
            <column default="0" name="istemplate" />
        </property>
        <property name="typeValue" type="integer">
            <column default="1" name="typevalue" />
        </property>
        <property name="gstrType" type="integer">
            <column default="0" name="gstrtype" />
        </property>
        <property name="partlyJeEntryWithCnDn" type="integer">
            <column default="0" name="partlyjeentrywithcndn" />
        </property>
        <property name="templatepermcode" type="long"> 
            <column name="templatepermcode"/>
        </property>
        <property name="revalInvoiceId" type="string">
            <column name="revalinvoiceid"/>
        </property>
        <property name="itcTransactionIds" type="string">
            <column name="itctransactionids"/>
        </property>
        <property name="badDebtJE" type="boolean">
            <column name="isbaddebtje"/>
        </property>
        <property name="taxAdjustmentJE" type="boolean">
            <column name="istaxadjustmentje"/>
        </property>
        <property name="seqnumber" type="integer">
            <column name="seqnumber" default='0'/>
        </property>
        <property name="datePreffixValue" type="string">
            <column length="50" name="datepreffixvalue"/>
        </property>
        <property name="dateSuffixValue" type="string">
            <column length="50" name="datesuffixvalue"/>
        </property>
        <property name="dateAfterPreffixValue" type="string">
            <column length="50" name="dateafterpreffixvalue"/>
        </property>
        <property name="approvestatuslevel" type="integer">
            <column name="approvestatuslevel" not-null="false" default='11'/>
        </property>
        <property name="gstCurrencyRate" type="double">
            <column  name="gstcurrencyrate" default="0"/>
        </property>
        <property name="badDebtSeqNumber" type="string">
            <column  name="baddebtseqnumber"/>
        </property>
        <property name="customer" type="string"> 
            <column length="255" name="customer"/>
        </property>
        <property name = "paymentcurrencytopaymentmethodcurrencyrate" type="double" >
            <column name="paymentcurrencytopaymentmethodcurrencyrate" default="1"/>
        </property>
        <property name="ismulticurrencypaymentje" type="true_false">
            <column name="ismulticurrencypaymentje"/>
        </property>
        <property name="isexchangegainslossje" type="boolean">
            <column name="isexchangegainslossje" default="false"/>
        </property>
        <property name="draft" type="boolean">
            <column name="isdraft"/>
        </property>
        <property name="isDishonouredCheque" type="true_false">
            <column name="isdishonouredcheque" default="'F'" />
        </property>
        <property name="transactionId" type="string">
            <column name="transactionId" length="255"/>
        </property>
        <property name="transactionModuleid" type="integer">
            <column name="transactionModuleid" default="0"/>
        </property>
        <property name="toIncludeInGSTReport" type="true_false">
            <column name="toincludeingstreport" default="false" />
        </property>
        <many-to-one class="com.krawler.hql.accounting.SequenceFormat" column="seqformat" name="seqformat"/>
        <many-to-one column="accjecustomdataref" class="com.krawler.hql.accounting.AccJECustomData"  name="accBillInvCustomData" />
        <many-to-one class="com.krawler.hql.accounting.JournalEntry" column="parentje" name="parentJE"/>
        <many-to-one cascade="all" class="com.krawler.hql.accounting.RepeatedJE" column="repeateje" name="repeateJE" />
        <many-to-one class="com.krawler.hql.accounting.BankReconcilationDraft" column="reconciliationdraft" name="reconciliationDraft" />
        <many-to-one class="com.krawler.common.admin.CostCenter" name="costcenter"/>
        <many-to-one class="com.krawler.common.admin.Company" name="company" not-null="true"/>
        <many-to-one class="com.krawler.common.admin.KWLCurrency" column="currency" name="currency"/>
        <many-to-one class="com.krawler.hql.accounting.Cheque" column="cheque" name="cheque"/>
        <many-to-one class="com.krawler.common.admin.User" column="createdby" name="createdby"/>
        <many-to-one class="com.krawler.hql.accounting.MasterItem" name="paidTo" column="paidto"/>
        <many-to-one class="com.krawler.hql.accounting.PaymentMethod" name="paymentMethod" column="paymentmethod"/>
    </class>
</hibernate-mapping>
