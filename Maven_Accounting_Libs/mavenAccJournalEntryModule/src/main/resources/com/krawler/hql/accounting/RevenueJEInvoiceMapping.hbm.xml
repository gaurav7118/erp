<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.krawler.hql.accounting.RevenueJEInvoiceMapping" table="revenuejeinvoicemapping">
        <id name="id" column="id">
            <generator class="uuid"/>
        </id>
        <property name="InvoiceId" type="string">
            <column name="invoiceid" length="255"/>
        </property>
        <property name="JournalEntryId" type="string">
            <column name="jeid" length="255"/>
        </property>
    </class>
</hibernate-mapping>
