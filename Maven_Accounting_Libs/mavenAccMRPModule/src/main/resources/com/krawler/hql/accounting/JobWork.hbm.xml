<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.krawler.spring.mrp.jobwork.JobWork" table="mrp_job_order">
    <id column="id" name="id">
      <generator class="uuid"/>
    </id>
    <property name="jobordername" type="string">
      <column name="jobordername"/>
    </property>
    <property name="jobordernumber" type="string">
      <column name="jobordernumber"/>
    </property>
    <property name="jobworkdate" type="date">
      <column name="jobworkdate"/>
    </property>
    <property name="dateofdelivery" type="date">
      <column name="dateofdelivery"/>
    </property>
    <property name="dateofshipment" type="date">
      <column name="dateofshipment"/>
    </property>
    <property name="excisedutychargees" type="double">
      <column name="excisedutychargees"/>
    </property>
    <property name="shipmentroute" type="string">
      <column name="shipmentroute"/>
    </property>
    <property name="gatepass" type="string">
      <column name="gatepass"/>
    </property>
    <property name="otherremarks" type="string">
      <column name="otherremarks"/>
    </property>
    <property name="productquantity" type="double">
      <column name="productquantity"/>
    </property>
    <property name="deleted" type="true_false">
      <column name="deleteflag" not-null="true"/>
    </property>
    <property name="seqnumber" type="integer">
      <column default="0" name="seqnumber"/>
    </property>
    <property name="datePreffixValue" type="string">
      <column length="50" name="datepreffixvalue"/>
    </property>
    <property name="dateSuffixValue" type="string">
      <column length="50" name="datesuffixvalue"/>
    </property>
    <property name="dateAfterPreffixValue" type="string">
            <column length="50" name="dateafterpreffixvalue"/>
    </property>
    <property name="autoGenerated" type="true_false">
      <column default="false" name="autogen" not-null="true"/>
    </property>
    <many-to-one class="com.krawler.common.admin.InventoryLocation" column="jobworklocation" name="jobworklocation"/>
    <many-to-one class="com.krawler.hql.accounting.SequenceFormat" column="seqformat" name="seqformat"/>
    <many-to-one class="com.krawler.hql.accounting.Vendor" column="vendorid" name="vendorid"/>
    <many-to-one class="com.krawler.common.admin.Company" column="company" name="companyid"/>
    <many-to-one class="com.krawler.common.admin.User" name="creator"/>
    <many-to-one class="com.krawler.common.admin.User" name="modifiedby"/>
    <many-to-one class="com.krawler.hql.accounting.Product" name="productid"/>
    <many-to-one class="com.krawler.spring.mrp.WorkOrder.WorkOrder" column="workorderid" name="workorderid"/>
    <many-to-one class="com.krawler.spring.mrp.jobwork.JobWorkCustomData" column="accjobworkcustomdataref" name="accJobWorkCustomData"/>
    
  </class>
</hibernate-mapping>
