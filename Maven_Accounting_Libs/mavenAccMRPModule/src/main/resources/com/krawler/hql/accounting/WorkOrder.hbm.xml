<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.krawler.spring.mrp.WorkOrder.WorkOrder" table="workorder">
        <id column="id" name="ID">
            <generator class="uuid"/>
        </id>
        <many-to-one class="com.krawler.hql.accounting.SequenceFormat" column="seqformat" name="seqformat"/>
        <property name="workOrderName" type="string">
            <column length="50" name="workordername" not-null="true"/>
        </property>
        <property name="workOrderID" type="string">
            <column length="50" name="workorderid" not-null="true"/>
        </property>
        <property name="projectId" type="string">
            <column length="255" name="projectid"/>
        </property>
        <property name="dateOfDelivery" type="date">
            <column name="dateofdelivery" not-null="false"/>
        </property>
         <property name="workOrderDate" type="date">
            <column name="workorderdate" not-null="false"/>
        </property>
        <property name="routeCode" type="string">
            <column length="50" name="routecode" />
        </property>
        <set cascade="all" inverse="true" name="labourmapping">
            <key column="workorderid"/>
            <one-to-many class="com.krawler.spring.mrp.WorkOrder.WorkOrderLabourMapping" />
        </set>
        <property name="deleted" type="true_false">
            <column name="deleted"/>
        </property>
        <property name="quantity" type="double">
            <column name="quantity"/>
        </property>
        <property name="producedquantity" type="double">
            <column name="producedquantity"/>
        </property>
        <set name="machinemapping" cascade="all" inverse="true">
            <key column="workorderid" />
            <one-to-many class="com.krawler.spring.mrp.WorkOrder.WorkOrderMachineMapping"/>
        </set>   
         <set name="workcentermapping" cascade="all" inverse="true">
            <key column="workorderid" />
            <one-to-many class="com.krawler.spring.mrp.WorkOrder.WorkOrderWorkCenterMapping"/>
        </set>   
        <property name="seqnumber" type="integer">
            <column default="0" name="seqnumber"/>
        </property>
        <property name="datePreffixValue" type="string">
            <column length="50" name="datepreffixvalue"/>
        </property>
        <property name="dateSuffixValue" type="string">
            <column length="50" name="datesuffixvalue"/>
        </property>
        <property name="dateAfterPreffixValue" type="string">
            <column length="50" name="dateafterpreffixvalue"/>
        </property>
        <property name="autoGenerated" type="true_false">
            <column default="false" name="autogen" not-null="true"/>
        </property>
        <property name="routingMasterType" type="integer">
            <column  name="routingmastertype"/>
        </property>
         <property name="fromlinktype" type="integer">
            <column  name="fromlinktype"/>
        </property>
        <set cascade="all" inverse="true" name="componentDetails">
            <key column="workorder"/>
            <one-to-many class="com.krawler.spring.mrp.WorkOrder.WorkOrderComponentDetails"/>
        </set>
        <many-to-one class="com.krawler.hql.accounting.SalesOrder" column="salesorder" name="salesOrder"/>
        <many-to-one class="com.krawler.spring.mrp.routingmanagement.RoutingTemplate" column="routetemplate" name="routeTemplate"/>
        <many-to-one class="com.krawler.hql.accounting.BOMDetail" column="bomid" name="bomid"/>
        <many-to-one class="com.krawler.hql.accounting.Customer" column="customer" name="customer"/>
        <many-to-one class="com.krawler.common.admin.User" column="createdby" name="createdBy"/>
        <many-to-one class="com.krawler.common.admin.User" column="modifiedby" name="modifiedBy"/>
        <many-to-one class="com.krawler.hql.accounting.MasterItem" name="workOrderStatus" column="workorderstatus"/>
        <many-to-one class="com.krawler.hql.accounting.MasterItem" name="workOrderType" column="workordertype"/>
        <many-to-one class="com.krawler.hql.accounting.Product" name="productID" column="productid"/>
        <many-to-one class="com.krawler.common.admin.Company" name="company" not-null="true"/>
        <many-to-one class="com.krawler.common.admin.InventoryWarehouse" name="orderWarehouse" column="orderwarehouse"/>
        <many-to-one class="com.krawler.common.admin.InventoryLocation" name="orderLocation" column="orderlocation"/>
        <many-to-one class="com.krawler.hql.accounting.Contract" name="salesContractID" column="salescontractid"/>
        <many-to-one class="com.krawler.hql.accounting.JournalEntry" column="closewoje" name="closeWOJE"/>
        <many-to-one class="com.krawler.hql.accounting.JournalEntryDetail" column="totalinventoryjedetail" name="totalinventoryJEdetail"/>
        <many-to-one column="accworkordercustomdataref" class="com.krawler.spring.mrp.WorkOrder.WorkOrderCustomData"  name="accWorkOrderCustomData" />
    </class>
</hibernate-mapping>
