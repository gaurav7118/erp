<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.krawler.hql.accounting.SalesOrder" table="salesorder">
        <id column="id" name="ID">
            <generator class="uuid"/>
        </id>
        <property name="salesOrderNumber" type="string">
            <column length="50" name="sonumber" not-null="true"/>
        </property>
        <property name="autoGenerated" type="true_false">
            <column default="false" name="autogen" not-null="true"/>
        </property>
        <property name="orderDate" type="timestamp">
            <column name="orderdate" not-null="true"/>
        </property>
        <property name="dueDate" type="timestamp">
            <column name="duedate" not-null="true"/>
        </property>
        <property name="favourite" type="boolean">
            <column name="favouriteflag" not-null="false" default="false"/>
        </property>
        <property name="printed" type="boolean">
            <column name="printedflag" not-null="false" default="false"/>
        </property>
        <property name="isEmailSent" type="boolean">
            <column name="emailsentflag" not-null="false" default="false"/>
        </property>
        <property name="memo" type="string">
            <column length="2048" name="memo"/>
        </property>
        <property name="deleted" type="true_false">
            <column name="deleteflag" not-null="true"/>
        </property>
        <property name="freeze" type="true_false">
            <column name="freezeflag" default="false"/>
        </property>
        <property name="pendingapproval" type="integer">
            <column default="0" name="pendingapproval" not-null="true"/>
        </property>
        <property name="istemplate" type="integer">
            <column default="0" name="istemplate" />
        </property>
        <property name="postText" type="string">
            <column  name="posttext"/>
        </property>
        <property name="shipvia" type="string">
            <column length="255" name="shipvia"/>
        </property>
        <property name="fob" type="string">
            <column length="255" name="fob"/>
        </property>
        <property name="shipdate" type="timestamp">
            <column name="shipdate"/>
        </property>
        <property name="approvallevel" type="integer">
            <column default="0" name="approvallevel" />
        </property>
        <property name="billTo" type="string">
            <column length="255" name="billto"/>
        </property>
        <property name="shipTo" type="string">
            <column length="255" name="shipto"/>
        </property>
        <property name="linkflag" type="integer" >
            <column name="linkflag" default="0" ></column>
        </property>
        <property name="perDiscount" type="true_false">
            <column name="perdiscount"/>
        </property>
        <property name="discount" type="double">
            <column name="discount" not-null="false"/>
        </property>
        <property name="invoicetype" type="string">
            <column  name="invoicetype"/>
        </property>
        <property name="createdon" type="long">
            <column name="createdon" not-null="true"/>
        </property>
        <property name="shiplength" type="double">
            <column default="1" name="shiplength" />
        </property>
        <property name="applyTaxToTerms" type="true_false">
            <column name="applytaxtoterms" default="'F'" />
        </property>
        <property name="updatedon" type="long">
            <column name="updatedon" not-null="true"/>
        </property>
        <property name="isOpeningBalanceSO" type="boolean">
            <column  name="isopeningbalenceso"/>
        </property>
    
        <property name="lockquantityflag" type="boolean">
            <column  name="lockquantityflag"/>
        </property>
        <property name="autoapproveflag" type="boolean">
            <column  name="autoapproveflag"/>
        </property>
    
        <property name="leaseOrMaintenanceSO" type="integer">
            <column name="leaseormaintenanceso"/>
        </property>

        <property name="gstIncluded" type="true_false">
            <column name="gstincluded" default="'F'" />
        </property>
        <property name="seqnumber" type="integer">
            <column name="seqnumber" default='0'/>
        </property>
        <property name="datePreffixValue" type="string">
            <column length="50" name="datepreffixvalue"/>
        </property>
        <property name="dateSuffixValue" type="string">
            <column length="50" name="datesuffixvalue"/>
        </property>
        <property name="dateAfterPreffixValue" type="string">
            <column length="50" name="dateafterpreffixvalue"/>
        </property>
        <property name="isReplacementSO" type="boolean">
            <column name="isreplacementso"/>
        </property>
        <property name="isconsignment" type="true_false">
            <column name="isconsignment" default="'F'" />
        </property>
        <property name="isMRPSalesOrder" type="true_false">
            <column name="ismrpsalesorder" default="'F'" />
        </property>
        <property name="isJobWorkOrder" type="true_false">
            <column name="isjobworkorder" default="'F'" />
        </property>
        <property name="externalCurrencyRate" type="double">
            <column name="externalcurrencyrate" default="0"/>
        </property>
        <property name="termsincludegst" type="true_false">
            <column name="termsincludegst"/>
        </property>
        <property name="fromdate" type="timestamp">
            <column name="fromdate"/>
        </property>
        <property name="todate" type="timestamp">
            <column name="todate"/>
        </property>
        <property name="customerPORefNo" type="string">
            <column length="50" name="customerporefno"/>
        </property>
        <property name="totalProfitMargin" type="double">
            <column default="0" name="totalprofitmargin" />
        </property>
        <property name="totalProfitMarginPercent" type="double">
            <column default="0" name="totalprofitmarginpercent" />
        </property>
        <property name="approvestatuslevel" type="integer">
            <column name="approvestatuslevel" not-null="false" default='11'/>
        </property>
        <property name="generatedSource" type="integer">
            <column name="generatedsource" not-null="false" default='0'/>
        </property>
        <property name="totalamount" type="double">
            <column  name="totalamount"/>
        </property>
        <property name="totalamountinbase" type="double">
            <column  name="totalamountinbase"/>
        </property>
        <property name="roundingadjustmentamountinbase" type="double">
            <column  name="roundingadjustmentamountinbase"/>
        </property>
        <property name="roundingadjustmentamount" type="double">
            <column  name="roundingadjustmentamount"/>
        </property>
        <property name="isRoundingAdjustmentApplied" type="true_false">
            <column name="isroundingadjustmentapplied" default="'F'" />
        </property>
         <property name="isDropshipDocument" type="true_false">
            <column name="isdropshipdocument" default="'F'" />
        </property>
        <property name="totallineleveldiscount" type="double">
            <column  name="totallineleveldiscount"/>
        </property>
        <property name="discountinbase" type="double">
            <column  name="discountinbase"/>
        </property>
        <property name="isopen" type="true_false">
            <column name="isopen" default="'T'" />
        </property>
        <property name="disabledSOforPO" type="true_false">
            <column name="disabledsoforpo" default="'F'"/>
        </property>
        <property name="isSOClosed" type="true_false">
            <column name="issoclosed" default="'F'"/>
        </property>
        <property name="formtype" type="string">
            <column  name="formtype"/>
        </property>
        <property name="totalShippingCost" type="double">
            <column default="0" name="totalshippingcost"/>
        </property>
        <many-to-one class="com.krawler.hql.accounting.SequenceFormat" column="seqformat" name="seqformat"/>
        <property name="isIndGSTApplied" type="true_false">
            <column name="isindgstapplied" default="'F'" />
        </property>
        <property name="isDraft" type="boolean">
            <column name="isdraft"/>
        </property>
        <property name="rcmapplicable" type="true_false">
            <column name="rcmapplicable" default="'F'" />
        </property>
        <property name="isMerchantExporter" type="true_false">
            <column name="ismerchantexporter" default="'F'" />
        </property>
        <property name="linkedPOBlocked" type="true_false">
            <column name="linkedpoblocked"/>
        </property>
        <set cascade="all" inverse="true" name="rows">
            <key column="salesorder"/>
            <one-to-many class="com.krawler.hql.accounting.SalesOrderDetail"/>
        </set>
        <many-to-one class="com.krawler.hql.accounting.Term" name="term"/>
        <many-to-one column="salesordercustomdataref" class="com.krawler.hql.accounting.SalesOrderCustomData"  name="soCustomData" />
        <many-to-one class="com.krawler.hql.accounting.Tax" column="tax" name="tax"/>
        <many-to-one class="com.krawler.common.admin.KWLCurrency" column="currency" name="currency"/>
        <many-to-one class="com.krawler.common.admin.CostCenter" name="costcenter"/>
        <many-to-one cascade="all" class="com.krawler.hql.accounting.Customer" column="customer" name="customer" not-null="true"/>
        <many-to-one class="com.krawler.common.admin.Company" name="company" not-null="true"/>
        <many-to-one class="com.krawler.common.admin.Projreport_Template" name="templateid"/>
        <many-to-one class="com.krawler.common.admin.User" column="approver" name="approver"/>
    
        <many-to-one class="com.krawler.common.admin.User" column="createdby" name="createdby"/>    
        <many-to-one class="com.krawler.common.admin.User" column="modifiedby" name="modifiedby"/>
    
        <many-to-one class="com.krawler.hql.accounting.MasterItem" name="salesperson" column="salesperson"/>
        <many-to-one class="com.krawler.hql.accounting.SalesOrder" column="parentso" name="parentSO"/>
        <many-to-one cascade="all" class="com.krawler.hql.accounting.RepeatedSalesOrder" column="repeateso" name="repeateSO" unique="true"/>
        <many-to-one class="com.krawler.common.admin.BillingShippingAddresses" column="billingshippingaddresses" name="billingShippingAddresses"/>
        <many-to-one class="com.krawler.hql.accounting.Contract" column="contract" name="contract"/>
        <many-to-one class="com.krawler.hql.accounting.Maintenance" column="maintenance" name="maintenance"/>
        <many-to-one class="com.krawler.common.admin.InventoryWarehouse" name="custWarehouse"/>
        <many-to-one class="com.krawler.common.admin.InventoryWarehouse" name="requestWarehouse" column="requestwarehouse"/>
        <many-to-one class="com.krawler.common.admin.InventoryLocation" name="requestLocation" column="requestlocation"/>
        <many-to-one class="com.krawler.hql.accounting.MasterItem" column="movementtype" name="MovementType"/>
    </class>
</hibernate-mapping>
