<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.krawler.common.admin.EditHelp" table="helpedit">
    <id name="id" type="string">
      <column name="id"/>
      <generator class="assigned"/>
    </id>
    <property name="modulename" type="string">
      <column default="" name="modulename"/>
    </property>
    <property name="compname" type="string">
      <column name="compname"/>
    </property>
    <property name="modeid" type="string">
      <column default="" name="modeid"/>
    </property>
    <property name="title" type="string">
      <column default="" name="title"/>
    </property>
    <property name="descp" type="string">
      <column default="" name="descp"/>
    </property>
    <property name="compid" type="string">
      <column default="" name="compid"/>
    </property>
  </class>
</hibernate-mapping>
