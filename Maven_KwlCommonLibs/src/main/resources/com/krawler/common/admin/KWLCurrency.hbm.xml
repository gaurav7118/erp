<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.krawler.common.admin.KWLCurrency" table="currency">
    <id column="currencyid" name="currencyID">
      <generator class="uuid"/>
    </id>
    <property name="name" type="string">
      <column length="100" name="name"/>
    </property>
    <property name="afterDecimalName" type="string">
      <column length="100" name="afterdecimalname"/>
    </property>
    <property name="htmlcode" type="string">
      <column length="30" name="htmlcode"/>
    </property>
    <property name="symbol" type="string">
      <column length="30" name="symbol"/>
    </property>
    <property name="currencyCode" type="string">
      <column length="30" name="currencycode"/>
    </property>
  </class>
</hibernate-mapping>
